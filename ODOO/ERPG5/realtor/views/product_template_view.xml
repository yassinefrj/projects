<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="realtor.product_view_type" model="ir.ui.view">
        <field name="name">product.template.product.form</field>
        <field name="priority">6</field>
        <field name="model">product.template</field>
        <field name="arch" type="xml">
            <form string="Produits">
                <sheet>
                    <group>
                        <field name="id" />
                        <field name="name" />
                        <field name="appartement_id" />
                        <field name="price" />
                        <field name="type" />
                        <field name="owner_id" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>